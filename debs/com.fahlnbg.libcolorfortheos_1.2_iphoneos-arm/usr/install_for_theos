
if [ -f "/usr/lib/libcolorpicker.dylib" ];then
cp -R /usr/lib/libcolorpicker.dylib /var/theos/lib/libcolorpicker.dylib
    else echo Pls !!! Install Libcolorpicker on Bigboss
fi

if [ -f "/var/theos/include/libcolorpicker.h" ];then
    rm -rf "/var/theos/include/libcolorpicker.h"
    fi
mv /usr/libcolorpicker.h /var/theos/include/
echo Done install..!!!!
